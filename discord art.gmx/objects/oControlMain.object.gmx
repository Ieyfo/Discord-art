    if(mouse_check_button(mb_left)){
        global.selected = ds_grid_get(oColorSelect.gridSelect,_colX,_colY);
        sSetUse(global.selected);
    }
}

// Check if mouse is within the edit area.
if(point_in_rectangle(mouse_x, mouse_y, 358, 100, 920, 665)){
    // Snap mouse to 32x32 grid while in edit area.
    with(oCursor){
        x = (mouse_x div 33) * 33;
        y = (mouse_y div 33) * 33;
    }
    
    // Adding a color to the edit area.
    if(mouse_check_button(mb_left) &amp;&amp; !position_meeting(mouse_x, mouse_y, oColor)){
        show_debug_message("Instance_created.")
        var obj = instance_create(oCursor.x, oCursor.y, global.inUse);
        with(obj){
            move_snap(33,33);
            x-=4;
            y+=1;
        }
    }
    
    // Removing a color from the edit area.
    if(mouse_check_button(mb_right)){
        var obj = collision_point(mouse_x, mouse_y, oColor, false, true);
        with(obj){
            instance_destroy();
        }
    }
}
